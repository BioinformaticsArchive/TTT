Project (TTTNewGUI)

set(CMAKE_INCLUDE_CURRENT_DIR ON)
set(CMAKE_AUTOMOC ON)
FIND_PACKAGE(Qt5Widgets REQUIRED)
# some .ui files sqlnewprojectdialog.ui sqlopenprojectdialog.ui
SET(TTTNewGUI_UIS tttmainwindow.ui 
    )

# another list, this time it includes all header files that should be treated with moc
SET( TTTNewGUI_MOC_HDRS
  tttmainwindow.h
  )

SET(TTTGUI_SRCS
  tttmainwindow.cpp
   )

# this will run uic on .ui files:

qt5_wrap_ui(TTTNewGUI_UI_HDRS tttmainwindow.ui)
add_executable(TTTNewGui main.cpp tttmainwindow.cpp)
qt5_use_modules(TTTNewGui Widgets)

